<%= form_with(model: [:admin_room, member, membership], local: true) do |form| %>
  <%= render 'shared/errors', errors: membership.errors %>
  <div class="form-group">
    <%= form.label :membershiped_at %>
    <%= form.datetime_local_field :membershiped_at, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= form.label :membership_form_id %>
    <%= form.collection_select(:membership_form_id, MembershipForm.all, :id, :title, { include_blank: true }, { class: 'form-control' }) %>
  </div>
  <%= form.submit t('actions.save'), class: 'btn btn-primary' %>
<% end %>
